  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "eslint": "^7.28.0",
    "mocha": "^9.0.0",
    "n-assert": "^7.0.0",
    "nyc": "^15.1.0",
    "rollup": "^2.48.0",
    "should": "^13.2.3",
