{
  "name": "http-z",
  "version": "6.1.1",
  "description": "HTTP message parser and builder",
  "engines": {
    "node": ">=12",
    "npm": ">=7"
  },
  "main": "index.js",
  "module": "index.esm.js",
  "typings": "index.d.ts",
  "files": [
    "src",
    "index.js",
    "index.d.ts",
    "index.esm.js"
