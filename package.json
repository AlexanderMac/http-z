{
  "name": "http-z",
  "version": "6.1.2",
  "description": "HTTP message parser and builder",
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "main": "index.js",
  "module": "index.esm.js",
  "typings": "index.d.ts",
  "files": [
    "src",
    "index.js",
    "index.d.ts",
    "index.esm.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "prettify": "prettier './src/**/*.js' './test/**/*.js' --write",
    "lint": "eslint --ext .js src/ test/",
    "test": "NODE_ENV=test mocha",
    "coverage": "nyc npm test",
    "coverage-gh": "nyc --reporter=lcov npm test && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexanderMac/http-z"
  },
  "keywords": [
    "http",
    "http-utility",
    "http-message",
    "builder",
    "parser",
    "request",
    "response"
  ],
  "author": "Alexander Mac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderMac/http-z/issues"
  },
  "homepage": "https://github.com/AlexanderMac/http-z#readme",
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "eslint": "^8.51.0",
    "mocha": "^10.2.0",
    "n-assert": "^7.0.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.3",
    "rollup": "^4.0.2",
    "should": "^13.2.3",
    "sinon": "^16.1.0"
  }
}
